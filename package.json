{
  "name": "brand-of-sacrifice",
  "author": "jyntran",
  "version": "1.0.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.3"
  },
  "pebble": {
    "displayName": "Brand of Sacrifice",
    "uuid": "9e7661b3-d9b1-48ac-9f09-cb0e6cf52119",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "diorite"
    ],
    "watchapp": {
      "watchface": true
    },
    "messageKeys": [
      "FontStyle",
      "BackgroundColour",
      "BrandColour",
      "TimeColour",
      "BatteryColour",
      "BatteryOutline",
      "BluetoothColour",
      "BluetoothVibration"
    ],
    "resources": {
      "media": [
        {
          "file": "images/brand-25.png",
          "name": "IMAGE_BRAND_25",
          "storageFormat": "png",
          "type": "bitmap",
          "menuIcon": true
        },
        {
          "characterRegex": "[:0-9/ ]",
          "type": "font",
          "name": "FONT_OLDEENGLISH_48",
          "file": "fonts/OldeEnglish.ttf"
        },
        {
          "characterRegex": "[:0-9/ ]",
          "type": "font",
          "name": "FONT_BLACKFOREST_42",
          "file": "fonts/BlackForest.ttf"
        },
        {
          "characterRegex": "[:0-9/ ]",
          "type": "font",
          "name": "FONT_NEWROCKER_42",
          "file": "fonts/NewRocker.ttf"
        }
      ]
    }
  }
